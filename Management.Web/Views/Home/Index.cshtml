            success: function (data) {
                var htmlstart = "<option>- Chọn nơi đi -</option>";
                var htmlend = "<option>- Chọn nơi đến -</option>";
                for (var i = 0; i < data.length; i++) {
                    htmlstart += "<option value='" + data[i].StartPoint + "'>" + data[i].StartPoint + "</option >";
                    htmlend += "<option value='" + data[i].EndPoint + "'>" + data[i].EndPoint + "</option >";
