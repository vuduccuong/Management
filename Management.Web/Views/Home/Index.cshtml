        //lấy danh sách xe theo điểm đón trả

        $("#btnSearchCar").click(function () {
            var StartPoint = $("#StartPoint option:selected").text();
            var EndPoint = $("#EndPoint option:selected").text();

            var time = 0;
            var date = $("#datetimea").val();

            var datepare = new Date(date);
            var mili = datepare.getTime();

            var timess = new Date();

            var mili2 = timess.getTime();

            if (mili < mili2) {
                $("#showCar").empty();
                $("#showCar").append("Ngày đi không hợp lệ");
            }
            else {
                var data = { startPoint: StartPoint, endPoint: EndPoint, dateBook: date, timeStart: time };
                $.ajax({
                    url: '/api/car/getcarbypoint',
                    type: 'get',
                    data: data,
                    success: function (data) {
                        $("#showCar").empty();
                        if (data.length == 0) {
                            $("#showCar").append("Không tìm thấy xe thoả mãn");
                        }
                        else {
                            console.log(data);
                            binData(data);
                            $("#showCar").show();

                        }
                    }
                });
            }
        });

